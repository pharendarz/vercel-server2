{
  "name": "vercel-server2",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "nodemon src/index.ts",
    "start:watch": "nodemon",
    "build": "rimraf dist && tsc",
    "ts.check": "tsc --project tsconfig.json",
    "add-build": "git add dist",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "pre-commit": [
    "ts.check",
    "build",
    "add-build"
  ],
  "nodemonConfig": {
    "ignore": [
      "**/*.test.ts",
      "**/*.spec.ts",
      ".git",
      "node_modules"
    ],
    "watch": [
      "./"
    ],
    "exec": "npm start",
    "ext": "ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pharendarz/vercel-server2.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/pharendarz/vercel-server2/issues"
  },
  "homepage": "https://github.com/pharendarz/vercel-server2#readme",
  "dependencies": {
    "@types/express": "^4.17.3",
    "@types/express-session": "^1.17.3",
    "@types/mongodb": "^3.6.8",
    "@types/mongoose": "^5.10.3",
    "express": "^4.19.2",
    "nodemon": "^3.1.0",
    "pre-commit": "^1.2.2",
    "rimraf": "^5.0.5",
    "vercel": "^34.0.0"
  },
  "devDependencies": {
    "@types/node": "^13.9.0",
    "mongoose": "^5.11.18",
    "ts-node": "^8.9.0",
    "tslint": "^6.1.0",
    "typescript": "^5.4.5"
  }
}
